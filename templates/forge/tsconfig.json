{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"paths": {
			"@/*": [
				"./src/*"
			]
		}
	}
}
